if %errorlevel%==2 goto :mag
if %errorlevel%==3 goto :lovec
if %errorlevel%==4 goto :pekar
if %errorlevel%==5 goto :rybar
if %errorlevel%==6 goto :prevoznik

:kovar

cls
echo.
