@echo off
title SMV obchod

:obchod

cls
echo 1. Kovar
echo 2. Mag
echo 3. Lovec
echo 4. Pekar
echo 5. Rybar
echo 6. Prevoznik
choice /c 123456 /m "Ke komu chces jit ?"
